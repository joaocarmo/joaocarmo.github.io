  "scripts": {
    "build": "npm run clean && npm run build-prd",
    "build-dev": "NODE_ENV=development webpack --mode development",
    "build-prd": "NODE_ENV=production webpack --mode production",
    "clean": "rm -rf css/ js/ img/ fonts/ favicon.ico index.html service* precache* icon* manifest* workbox*",
    "start": "NODE_ENV=development webpack-dev-server --mode development",
    "test": "jest --passWithNoTests",
    "no-non-ascii": "scripts/no-non-ascii",
    "add-all": "git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run no-non-ascii && npm run build && npm test && npm run add-all"
    }
  },
